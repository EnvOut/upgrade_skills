<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
    <bean id="oracle" class="com.tow.spring.xml.BookwormOracle"/>
    <bean id="oracle" class="com.tow.spring.BookwormOracle"/>
    <bean id="collectionInjection" class="com.tow.spring.xml.CollectionInjection">
        <property name="mapFiled">
            <map>
                <entry key="someValue" value="Hello World!"/>
                <entry key="someBean" value-ref="oracle"/>
            </map>
        </property>
        <property name="listFiled">
            <list>
                <value>Hello world!</value>
                <ref bean="oracle"/>
            </list>
        </property>
        <property name="propsFiled">
            <props>
                <prop key="firstName">Chris</prop>
                <prop key="secondName">Schaefer</prop>
            </props>
        </property>
        <property name="setFiled">
            <set>
                <value>Hello World</value>
                <ref bean="oracle"/>
            </set>
        </property>
    </bean>

</beans>